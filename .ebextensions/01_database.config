packages:
    yum:
        mysql: []
container_commands:


    
    00fix_permissions_dirs:
    command: "find . -type d -exec chmod 755 {} \;"
    ignoreErrors: true

    01fix_permissions_files:
    command: "find . -type f -exec chmod 644 {} \;"
    ignoreErrors: true
    
    import-database:
    command: ".ebextensions/database_import.sh"
    leader_only: true


       
